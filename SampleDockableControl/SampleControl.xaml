<UserControl x:Class="SampleDockableControl.SampleControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:SampleDockableControl"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    
    <UserControl.DataContext>
        <local:ControlViewModel/>
    </UserControl.DataContext>
    
    <Grid>
        <StackPanel>
            <Button 
                x:Name="getDocName"
                Content="Do something in revit"
                Click="getDocName_Click"/>
            <Button 
                x:Name="selectElement"
                Content="Select element sample"
                Click="selectElement_Click"/>
            <Button
                x:Name="changeDetailLevel"
                Content="Change Detail Level Sample"
                Click="changeDetailLevel_Click"/>
        </StackPanel>
    </Grid>
</UserControl>
